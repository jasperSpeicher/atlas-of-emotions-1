        },
        {
            "key": "anger_triggers_header_2",
            "value": "Interference with action"
        },
        {
            "key": "anger_triggers_header_3",
            "value": "Rejection by a loved one"
        },
        {
            "key": "anger_triggers_header_4",
            "value": "Inefficiency or bureaucracy"
        },
        {
            "key": "anger_triggers_header_5",
            "value": "Being put down by an authority figure"
        },
        {
            "key": "anger_triggers_header_6",
            "value": "Encountering offensive beliefs"
        },
        {
            "key": "anger_triggers_header_7",
            "value": "Being wrongfully accused"
        },
        {
            "key": "anger_moods_header",
            "value": "Irritable"
        },
        {
            "key": "anger_moods_body",
            "value": "A readiness, a need to become angry. Filtering perceptions to see provocations not noticed when not irritable."
        }
